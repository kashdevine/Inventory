services:
  inventory:
    build:
      context: D:\Microservices\Inventory\src\Inventory
      dockerfile: Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: inventory
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\Kash\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Kash\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'